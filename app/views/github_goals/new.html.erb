<div class='row panel github_panel'>
  <div class= 'github-username'>

      <h1>Enter your GitHub username</h1>
        <%= form_for @github_goal do |f| %>
          <%= f.label :username %>
          <%= f.text_field :username %>
          <%= f.submit 'Submit Username', class: 'button'%>
        <% end %>
  </div>
</div>
